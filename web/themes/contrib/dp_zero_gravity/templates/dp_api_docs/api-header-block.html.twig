{#
/**
 * @file
 * Default theme implementation for API header.
 *
 * Available variables:
 * - attributes: The attribute object for the header.
 * - title: The title of the API.
 * - version: The version of the API.
 * - tags: The tags on the API (API categories by default).
 * - content: The content of the header.
 *
 * @ingroup themeable
 */
#}

{% extends '@dp_api_docs/api-header-block.html.twig' %}
{% block api_header_title %}
  <h1 class="h1--page-title">{{ title }}<span class="version">{{ version }}</span></h1>
{% endblock %}

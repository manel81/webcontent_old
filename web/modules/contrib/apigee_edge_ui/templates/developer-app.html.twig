{#
/**
 * @file
 * Overridden theme implementation to display a developer app.
 *
 * Available variables:
 * - content: All app properties. Use {{ content }} to print them all,
 *   or print a subset such as {{ content.field_example }}. Use
 *   {{ content|without('field_example') }} to temporarily suppress the printing
 *   of a given child element.
 * - attributes: HTML attributes for the containing element.
 *
 * @see template_preprocess_developer_app()
 *
 * @ingroup themeable
 */
#}
<div {{ attributes }}>
  <div class="developer-app__details">
    <h2>{{ 'Details'|t }}</h2>
    <div class="developer-app__details-content">
      {{ content|without('credentials') }}
    </div>
  </div>
  <div class="developer-app__credentials">
    {{ content.credentials }}
  </div>
</div>

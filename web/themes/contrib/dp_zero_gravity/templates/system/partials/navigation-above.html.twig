{#
/**
 * @file
 * Partial for page header when navigation is above the header.
 *
 * Available variables:
 *   @see page.html.twig
 */
#}
{% block region_navigation %}
  {{ page.navigation }}
{% endblock %}
<header{{ header_attributes.addClass('zg-page-header--nav-above') }}>
  {% block region_header_first %}
    {{ page.header_first }}
  {% endblock %}
  {% block region_header_second %}
    {{ page.header_second }}
  {% endblock %}
</header>